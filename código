Var
   // Seção de Declarações das variáveis
   TotalGastoMes ,PagFunc, Manutencao, Anuncios: vetor [1 .. 12] de real
   x, i, Mes: inteiro
   Maior, Menor, MediaMes, TotalGastoAno, MediaPag, MediaManu, MediaAnunc, TotalPag, TotalManu, TotalAnunc: real

Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   escreval(" olá vamos começar o nosso procedimento e descobriremos os gastos da Weg")

   para Mes de 1 ate 12 faca
      PagFunc[Mes]<-50000+randi(300000)
      Manutencao[Mes]<-100000+randi(600000)
      Anuncios[Mes]<-25000+randi(45000)
      TotalGastoMes[Mes]<-PagFunc[Mes]+Manutencao[Mes]+Anuncios[Mes]
      TotalPag<-TotalPag+PagFunc[Mes]
      TotalManu<-TotalManu+Manutencao[Mes]
      TotalAnunc<-TotalAnunc+Anuncios[Mes]
      TotalGastoAno<-TotalGastoAno+TotalGastoMes[Mes]
      Mes<-Mes+1
   fimpara
   MediaPag<-TotalPag/12
   MediaManu<-TotalManu/12
   MediaAnunc<-TotalAnunc/12
   MediaMes<-TotalGastoAno/12
   Menor<-TotalGastoMes[1]
   para Mes de 2 ate 12 faca
      se (Menor > TotalGastoMes[Mes]) entao
         Menor<-(TotalGastoMes[Mes])
      fimse
   fimpara
   Maior<-TotalGastoMes[1]
   para Mes de 2 ate 12 faca
      se (Maior < TotalGastoMes[Mes]) entao
         Maior<-(TotalGastoMes[Mes])
      fimse
   fimpara
   
   escreval("agora temos todos os gastos medias e muito mais do ano que se passou")
   escreval("agora o programa vai listar as informações registradas e calculadas")
   escreval("o mês com menor gastos gastou no total ", Menor," reais")
   escreval("o mês os maiores gastos gastou no total ", Maior," reais")
   escreval("a média mensal de gastos foi de ", MediaMes," reais")
   escreval("os pagamentos para os funcionarios deu em média ", MediaPag," de reais por mes")
   escreval("a média mensal de gastos com anuncios deu ", MediaAnunc, " de reais")
   escreval("em manutenção foram gastos em média ", MediaManu," de reais")
   escreval("no total foram gastos mais de ", TotalGastoAno," de reais neste ano")

Fimalgoritmo
